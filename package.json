{
  "name": "create-mock-backend",
  "version": "0.0.8",
  "description": "This is npx starter package for installing your favourite backend template for mockBee.",
  "keywords": [
    "miragejs",
    "mock-server",
    "npx",
    "fake-api"
  ],
  "main": "dist/main.js",
  "bin": "bin.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-package": "rollup -c",
    "lint": "eslint ./",
    "lint-fix": "eslint ./ --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\""
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neogcamp/mockBee.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neogcamp/mockBee/issues"
  },
  "homepage": "https://github.com/neogcamp/mockBee#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "rollup": "^2.57.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "degit": "^2.8.4",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "path": "^0.12.7",
    "prompts": "^2.4.1"
  }
}
